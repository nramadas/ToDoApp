<div class="form">
  <%= form_for(@session, url: sessions_path, method: :post) do |f| %>
    <p>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </p>
    <p>
      <%= f.submit "Log In" %>
    </p>
  <% end %>
</div>